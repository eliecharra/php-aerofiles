{
  "name": "eliecharra/php-aerofile",
  "type": "library",
  "description": "Waypoint, task, tracklog readers for aviation",
  "keywords": ["aviation", "IGC", "paragliding"],
  "homepage": "https://github.com/eliecharra/php-aerofile",
  "license": "MIT",
  "authors": [
    {
      "name": "Elie CHARRA",
      "email": "elie.charra@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/eliecharra/php-aerofile/issues"
  },
  "require": {
    "php": "~7.3",
    "mjaschen/phpgeo": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Aerofiles\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Aerofiles\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0"
  },
  "scripts": {
    "test": "vendor/bin/phpunit --colors=always"
  }
}
